{
  "name": "runnable",
  "description": "Tiny little task runner",
  "version": "0.0.1",
  "main": "dist/runnable.js",
  "license": "MIT",
  "source": "src/index.js",
  "module": "dist/runnable.module.js",
  "unpkg": "dist/runnable.umd.js",
  "scripts": {
    "build": "microbundle",
    "test": "mocha src/**/*.test.js --require esm --require chai/register-expect",
