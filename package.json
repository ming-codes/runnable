    "test:debug": "mocha src/**/*.test.js --require esm --require chai/register-expect --watch --inspect-brk"
  },
  "volta": {
    "node": "12.16.2"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "mocha": "^7.1.1"
  },
  "devDependencies": {
    "microbundle": "^0.11.0"
  }
}
